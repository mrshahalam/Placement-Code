package com.codewithshah;
import java.lang.*;
import java.io.*;
import java.util.*;
public class fifth {


    public static void main(String[] args) {

        String input[] = "Geeks For shah".split(" ");
        String  s= "";

        for(int i= input.length-1;i>=0;i--)
        {
            s+=input[i]+" ";
        }
        System.out.println("Reversed String");
        System.out.println(s.substring(0,s.length()-1));

    }
}
